  object OD_houses: TOracleDataSet
    SQL.Strings = (
      'select t.rowid, t.house_type, t.id, t.kul, s.name, t.nd, '
      ' LTRIM(t.nd,'#39'0'#39') as n_nd, t.minlsk, t.maxlsk, t.uch, t.psch,'
      
        ' t.fk_pasp_org, o.name as pasp_name, t.fk_other_org, o2.name as ' +
        'other_name, t.kran1, t.k_lsk_id, t.k_lsk_id as fk_k_lsk,'
      ' t.FK_TYPESPAY, t.postcode, t.guid, t.is_private'
      
        ' from scott.c_houses t, scott.spul s, scott.t_org o, scott.t_org' +
        ' o2'
      
        'where t.kul=s.id and t.fk_pasp_org=o.id(+) and t.fk_other_org=o2' +
        '.id(+) and'
      '(:var1_=0 or (:var1_=1 and nvl(t.psch,0)<>1))'
      
        'and (:flt_reu_ is not null and exists (select * from scott.kart ' +
        'k where k.reu=:flt_reu_ and k.house_id=t.id)'
      ' or :flt_reu_ is  null)'
      
        'and (:flt_kul_ is not null and t.kul=:flt_kul_ or :flt_kul_ is  ' +
        'null)'
      'and (:flt_house_ <>-1 and t.id=:flt_house_ or :flt_house_ =-1)'
      
        'and (:flt_nd_ is not null and t.nd=:flt_nd_ or :flt_nd_ is  null' +
        ')')
